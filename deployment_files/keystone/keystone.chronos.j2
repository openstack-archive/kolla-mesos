{
    "name": "keystone-bootstrap",
    "mem": 32.0,
    "cpus": 0.3,
    "command": "kolla_start",
    "container": {
        "type": "DOCKER",
        "image": "{{ kolla_ansible_image }}:latest",
        "network": "HOST"
    },
    "environmentVariables": [
        {"name": "KOLLA_CONFIG_STRATEGY", "value": "{{ config_strategy }}"},
        {"name": "KOLLA_CONFIG", "value": "{{ kolla_config }}"},
        {"name": "KOLLA_GROUP", "value": "keystone"},
        {"name": "KOLLA_ROLE", "value": "keystone_bootstrap"},
        {"name": "KOLLA_ZK_HOSTS", "value": "{{ zookeeper_hosts }}"}]
}
